Sub downloadXml()
Dim d As clsXapDebug: Set d = New clsXapDebug
Dim f As IXMLDOMSelection
Dim sFol As String: sFol = ""
Dim sFrm As String: sFrm = ""
Dim sCid As String
sFol = "Menu1"
'sFrm = "MFG0001"

Set f = d.getFolder(sFol)

Call d.downloadXml(f)

'Dim l As Long
'For l = 0 To f.Length - 1
'Debug.Print f.Item(l).XML
'Next l

'uf1.out f.Context.XML

'Debug.Print f.Length

'uf1.out f.Item(0).XML

Set d = Nothing
Set f = Nothing
Set fr = Nothing

End Sub

Sub createFixFilter()

Dim n As IXMLDOMElement
Dim s As String

If Not ActiveSheet.Tab.ColorIndex = -4142 Then Stop

Set d = New clsXapDebug
Set n = d.createFixFilterXml()

End Sub

