<OBJ clid="ComboBox" Name="cbxSection" TextList="" Text="Select" TextFont="Meiryo UI,-13,0,400,0,0,0,0,0" Top="10" Left="234" Height="40" Width="236" Event="
event OnSelectChanged(index,text)
