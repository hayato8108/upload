" INNER JOIN " & _
"(SELECT [COL1],[COL3] FROM [TEST_OUTER$] )  AS [OUTER] ON [OUTER].[COL1] = [MAIN].[COL1] )" & _
""
    
    sSqlQuery = sTable1
    
