'FA-PANEL保存形式(XML)をスプレッドシートへダウンロードする
'FIXデータ形式(テキストファイル)をFA-PANEL形式へ変換する

Private InPath_ As String
Private OutPath_ As String
Private crrFolder_ As String
Private crrForm_ As String

Sub createPartsXmlShtByPGPRM()
Application.ScreenUpdating = False
Dim sPrfs As String: sPrfs = fKey2Val("PGPRM_CLT_EXE_ROOT", "CLT_EXE_CREATESHT", "LinkShtClt")
Dim sPrf As String: sPrf = inptPrf
Dim sLinkShtName As String: sLinkShtName = "LinkShtClt"
Dim sShtName As String
Dim sAttr As String
Dim sLibCls As String
Dim v As Variant
If UBound(Split(sPrfs, ",")) = 0 Then Call DebugOut("ERR", "sprfs length is 0")

For Each v In Split(sPrfs, ",")
    'append range
    sData = fGetRowData(ThisWorkbook.Name, sLinkShtName, CStr(v), "A")  'row => [link parameter]
    If UBound(sData) = -1 Then Call DebugOut("ERR", "fGetRowData fail : " & v & " " & sLinkShtName): Exit Sub
    sShtName = CStr(sData(1))
    sLibCls = CStr(sData(2))
    vRest = retArrayBetween(sData, 3, UBound(sData))
    Call SelectOrAddWs(sShtName, ThisWorkbook.Name)
    With ThisWorkbook.Sheets(sShtName)
        .Activate
        .Cells.Clear
        .Range(Cells(1, 1), Cells(Rows.Count, Columns.Count)).NumberFormatLocal = "@"
        .Range("A1").Interior.Color = RGB(255, 155, 155)
        .Range("A1").Value = "_Index"
        
        If (UBound(vRest)) = -1 Then Exit Sub
             .Rows(1).Clear
            Call PasteShtData(ThisWorkbook.Name, .Name, vRest)
            Call downloadPartsXml(.Name, sLibCls)
            Call PasteShtData(ThisWorkbook.Name, .Name, vRest)
    End With
Next v

If CBool(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_SORTSHT", sLinkShtName)) = True Then SortShtByPGPRM
If CBool(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_RTNSHT", sLinkShtName)) = True Then ThisWorkbook.Sheets(sLinkShtName).Activate
Application.ScreenUpdating = True
ThisWorkbook.Sheets("LinkShtClt").Activate
End Sub


Sub WsUploadNumberingXml()
Application.ScreenUpdating = False
Dim vDom As clsXapDebug: Set vDom = New clsXapDebug
Dim vBuffer(10) As String
Dim ws As Worksheet
On Error Resume Next

With ThisWorkbook.ActiveSheet
    .Activate
    vDom.uploadNumberingXml (ActiveSheet.Name)
End With

Set vDom = Nothing
Application.ScreenUpdating = True
End Sub


Sub WsdownloadFolderXml()
Application.ScreenUpdating = False
Dim d As clsXapDebug: Set d = New clsXapDebug
Dim f As IXMLDOMSelection
Dim sFol As String: sFol = ""
Dim sCid As String
Dim l As Long
Dim vBuffer(10) As String
Dim sOutWsName As String
sOutWsName = fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_OUT_SHT", "LinkShtDB")
On Error Resume Next

If sOutWsName = "" Then
   Call DebugOut("ERR", "empty XML_out_sht")
Else
    Call SelectOrAddWs(sOutWsName, ThisWorkbook.Name)
    Set ws = Sheets(sOutWsName)
End If
    ws.Activate

sFol = fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_LOAD_FOLDER", "LinkShtDB")
d.reloadPath
Set f = d.getFolder(sFol)

If f.Length = 0 Then MsgBox "Folder Load Error : " & sFol: Exit Sub
Call d.downloadFolderXml(f)
Call DebugOut("REM", "alter fgetpgval")

Set d = Nothing
Set f = Nothing
Set fr = Nothing

Application.ScreenUpdating = True
End Sub
        
Sub downloadPartsXml(inptShtName As String, inptLibComment As String)
Dim d As clsXapDomDoc: Set d = New clsXapDomDoc
Dim sLibCmt  As String: sLibCmt = inptLibComment
Dim oXmlFormSeletion As IXMLDOMSelection

d.reloadPath
Set oXmlFormNodes = d.selectNodesEX("//FORMROOT/OBJ")
     With ThisWorkbook.Worksheets(inptShtName)
        Call DebugOut("UTIL", " dounloadPartsXml: " & inptLibComment)
        For lFolderCnt = 0 To oXmlFormNodes.Length - 1
            Set oXmlFormNode = oXmlFormNodes(lFolderCnt)
            Set oXmlFormSeletion = d.getFolder(oXmlFormNode.Attributes.Item(1).NodeValue)
            Call downloadFolderXml(oXmlFormSeletion, sLibCmt)
        Next lFolderCnt
        .Columns.AutoFit
        .Range("A1").Activate
    End With
Set d = Nothing
Set oXmlFormSeletion = Nothing

End Sub


Sub downloadXmlOnClearSht(inptList As IXMLDOMSelection)
    Call SelectOrAddWs(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_OUT_SHT"), ThisWorkbook.Name)
    With ThisWorkbook.Worksheets(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_OUT_SHT"))
        .Range(Cells(2, 1), Cells(Rows.Count, Columns.Count)).Clear
        .Range(Cells(2, 1), Cells(Rows.Count, Columns.Count)).NumberFormatLocal = "@"
        .Range("A1").Interior.Color = RGB(255, 155, 155)
         Call downloadFolderXml(inptList)
        .Range("A1").Activate
    End With
End Sub


Sub downloadFolderXml(inptList As IXMLDOMSelection, Optional inptSearchKey As String = "")
Call DebugOut("SUB_ST", "downloadxml")

Dim sKey As String: sKey = inptSearchKey
Dim vFolders As IXMLDOMNodeList: Set vFolders = inptList
Dim vFolder As IXMLDOMElement
Dim vForms As IXMLDOMElement
Dim vObj As IXMLDOMElement
Dim vO As IXMLDOMElement
crrFolder_ = ""
crrForm_ = ""
Dim lCol As Long: lCol = 2
Dim lCnt As Long
Dim l As Long: l = 0

With ActiveSheet
    .Cells(CLng(.Cells(Rows.Count, 1).End(xlUp).Row) + 1, 1).Activate

    If vFolders Is Nothing Then Exit Sub
    For l = 0 To vFolders.Length - 1
        Set vFolder = vFolders.Item(l)
        crrFolder_ = vFolder.Attributes.getNamedItem("Name").Text
        Call DebugOut("UTIL", crrFolder_)
            For Each vForms In vFolder.ChildNodes
                sPrvForm = crrForm_
                If vForms.Attributes.getNamedItem("clid").Text = "Form" Then
                    crrForm_ = vForms.Attributes.getNamedItem("Name").Text
                    Call DebugOut("UTIL", crrForm_)
                End If
               
                    For Each vObj In vForms.ChildNodes

                        If Not IsNull(vObj.getAttribute("LibraryComment")) Then
                        
                            If vObj.Attributes.getNamedItem("LibraryComment").Text = sKey Then
                             ActiveCell.Value = ActiveCell.Row - 1 'Column A index
                                .Range("A1").Interior.Color = RGB(55, 155, 255)
                                Do While .Cells(1, lCol).Value <> "" 'field Attrs
                                    If Left(.Cells(1, lCol).Value, 1) = "%" Then
                                        'do nothing
                                    ElseIf .Cells(1, lCol).Value = "Folder" Then
                                        ActiveCell.Offset(0, lCol - 1) = crrFolder_
                                    ElseIf .Cells(1, lCol).Value = "Form" Then
                                       ActiveCell.Offset(0, lCol - 1) = crrForm_
                                    ElseIf .Cells(1, lCol).Value = "event" Then
                                      If Not IsNull(vObj.getAttribute(.Cells(1, lCol).Value)) Then ActiveCell.Offset(0, lCol - 1) = eventRegx(vObj.Attributes.getNamedItem(.Cells(1, lCol).Value).Text)
                                    ElseIf Not IsNull(vObj.getAttribute(.Cells(1, lCol).Value)) Then 'Attribute
                                        ActiveCell.Offset(0, lCol - 1) = vObj.Attributes.getNamedItem(.Cells(1, lCol).Value).Text
                                    End If
                                        lCol = lCol + 1
                                Loop
                                
                                lCnt = incIndex 'display number
                                lCol = 2 ' reset
                                sPrvFolder = crrFolder_
                                sPrvForm = crrForm_
                                ActiveCell.Offset(1, 0).Activate

                                If Not vObj.ChildNodes.Length = 0 And _
                                    CBool(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_CHILDELM_EXPAND", "LinkShtClt")) = CBool(1) Then
                                    For Each vO In vObj.ChildNodes
                                        Call parseGroup(vO, "", .Name)
                                    Next vO
                                End If
                                
                        End If
                    End If
                Next vObj
            Next vForms
        Next l

    .Columns.AutoFit
End With

Set vDoc = Nothing
Set vFolder = Nothing
Set vObj = Nothing
End Sub


Sub parseGroup(inpt As IXMLDOMElement, inptC As String, inptOutSht As String)
Dim vObj As IXMLDOMElement: Set vObj = inpt
Dim vO As IXMLDOMElement
Dim lCol As Long: lCol = 2
Dim sC As String: sC = "*" & inptC

With ThisWorkbook.Sheets(inptOutSht)
    ActiveCell.Value = ActiveCell.Row - 1 'Column A index
    Do While .Cells(1, lCol).Value <> "" 'field Attrs
        If .Cells(1, lCol).Value = "folder" Then
          ActiveCell.Offset(0, lCol - 1) = crrFolder_
        ElseIf .Cells(1, lCol).Value = "form" Then
           ActiveCell.Offset(0, lCol - 1) = crrForm_
        ElseIf .Cells(1, lCol).Value = "group" Then
        
           ActiveCell.Offset(0, lCol - 1) = sC
        ElseIf .Cells(1, lCol).Value = "event" Then
          If Not IsNull(vObj.getAttribute(.Cells(1, lCol).Value)) Then ActiveCell.Offset(0, lCol - 1) = eventRegx(vObj.Attributes.getNamedItem(.Cells(1, lCol).Value).Text)
        ElseIf Not IsNull(vObj.getAttribute(.Cells(1, lCol).Value)) Then 'Attribute
            ActiveCell.Offset(0, lCol - 1) = vObj.Attributes.getNamedItem(.Cells(1, lCol).Value).Text
        End If
            lCol = lCol + 1
    Loop
    lCnt = incIndex 'display number
    lCol = 2 ' reset
    ActiveCell.Offset(1, 0).Activate
    If Not vObj.ChildNodes.Length = 0 Then
        For Each vO In vObj.ChildNodes
            Call parseGroup(vO, sC, inptOutSht)
        Next vO
    End If

End With

Set vob = Nothing
End Sub


Function eventRegx(inptStr As String) As String
Dim sEventRegx As String: sEventRegx = "AddTag\(""?[,-_A-z0-9]+""?\)"
Dim sRslt As String: sRslt = ""
Dim i As Long
Dim oArr As Object: Set oArr = CreateObject("System.Collections.ArrayList")
Dim regx As Object: Set regx = CreateObject("vbscript.regexp")
Dim regxR As Object: Set regxR = CreateObject("vbscript.regexp")
Dim oMatch As Object
Dim oMatchR As Object

With regx
    .Global = True 'isMultiple
    .MultiLine = False
    .ignorecase = False
    .Pattern = sEventRegx
End With

With regxR
    .Global = True 'isMultiple
    .MultiLine = False
    .ignorecase = False
End With
    
If regx.test(inptStr) Then Set oMatch = regx.Execute(inptStr)

If oMatch Is Nothing Then
    eventRegx = ""
Else
    For i = 0 To oMatch.Count - 1
        sRslt = sRslt & IIf(sRslt = "", "", vbLf) & oMatch.Item(i)
        
       ' right hand side
       Dim s As String
       s = fReplace(oMatch.Item(i), "AddTag(", "")
       s = fReplace(s, ")", "")
       s = fReplace(s, """", "")
        regxR.Pattern = s & "\s?=.*"

        If regxR.test(inptStr) Then Set oMatchR = regxR.Execute(inptStr)
        If Not oMatchR Is Nothing Then sRslt = sRslt & "," & oMatchR.Item(0)
        
    Next i
End If

eventRegx = sRslt

End Function


Sub uploadNumberingXml(inptWs As String)
'Function createEllipseElm(inptT As Long, inptL As Long, inptH As Long, inptW As Long) As IXMLDOMElement
''<OBJ clid="Ellipse" RotateX="45" RotateY="45" Top="0" Left="0" Height="90" Width="90" LibraryComment="_Circle">
Dim vDom As clsXapDomDoc: Set vDom = clsXapDomDoc
Dim lOdfH As Long: lOdfH = 1200
Dim lOdfW As Long: lOdfW = 1920
Dim vForm As IXMLDOMElement: Set vForm = vDom.createFormElm(lOdfH, lOdfW)
Dim vFilter As IXMLDOMElement
Dim lCol As Long: lCol = 1
Dim s As String
Dim sKey As String
Dim lMatchCol As Long

With ThisWorkbook.Sheets(inptWs)
    .Range("A1").Interior.Color = RGB(255, 155, 155)
    .Activate
    If .Range("A2").Value = "" Then .Range("A3").Activate Else .Range("A2").Activate

    Do While ActiveCell.Value <> "" ' row direction
        .Range("A1").Interior.Color = RGB(55, 155, 255)
        Set vFilter = setBaseNumEll

        Do While .Cells(1, lCol).Value <> ""  'field Attrs <--->
            Select Case .Cells(1, lCol).Value
                Case "_Index"
                        Set vFilter = vDom.setAttributesEx(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Top"
                        Set vFilter = vDom.setAttributesEx(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Left"
                        Set vFilter = vDom.setAttributesEx(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Height"
                    If vDom.getAttributesEx(vFilter, "Top") = 0 Then Stop 'TMP
                        Set vFilter = vDom.setAttributesEx(vFilter, "Top", vDom.getAttributesEx(vFilter, "Top") + CStr(.Cells(ActiveCell.Row, lCol).Value) / 2)
                Case "Width"
                        If vDom.getAttributesEx(vFilter, "Left") = 0 Then Stop 'TMP
                        Set vFilter = vDom.setAttributesEx(vFilter, "Left", vDom.getAttributesEx(vFilter, "Left") + CStr(.Cells(ActiveCell.Row, lCol).Value) / 2)
                End Select
                lCol = lCol + 1

          Loop ' field Attrs

          
        vForm.appendChild vFilter
        lCol = 1

        ActiveCell.Offset(1, 0).Activate 'row direction
    Loop

    .Range("A1").Activate 'iterate end
End With

s = "<?xml version=""1.0""?>" & _
"<ROOT><FORMROOT clid=""Client"" Name=""Client"" ShowGrid=""F"" AdjustGrid=""F"" Product=""PA-Panel"" Ver=""6.0"" Rev=""12.1"" FileRev=""3"" FileSaveWithoutDefault=""T"">" & _
"<OBJ clid=""Folder"" Name=""Folder01"">" & _
"PLACEHOLDER" & _
"</OBJ>" & _
"</FORMROOT></ROOT>"

s = Replace(s, "PLACEHOLDER", vForm.XML)

Call SaveTextWithUtf8(s, OutPath_)

Set vDom = Nothing
Set vDoc = Nothing
Set vForm = Nothing
Set vFilter = Nothing

End Sub


Sub FIXREPO_EXE_CREATESHT()
Application.ScreenUpdating = False
Dim ws As Worksheet
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vDomDoc As DOMDocument60: Set vDomDoc = New DOMDocument60
Dim sLinkShtName As String: sLinkShtName = "LinkShtClt"
Dim vFixRepotFiles As FileDialog: Set vFixRepotFiles = Application.FileDialog(msoFileDialogFilePicker)
Dim vSelectedPict As FileDialogSelectedItems
Dim vCnt As Long
Dim vBuffer(10) As String
On Error Resume Next
Set vDomDoc = vDom.createNewXapDoc()
Set vDomDoc = vDomDoc.DocumentElement.ChildNodes(0).appendChild(vDom.createFolderElm())

With vFixRepotFiles
    .Filters.Clear
    .Filters.Add "textFile", "*.*", 1
    .AllowMultiSelect = True
    .Show
End With

If vFixRepotFiles.SelectedItems.Count = 0 Then Exit Sub

For vCnt = 1 To vFixRepotFiles.SelectedItems.Count ' 1 base array
    vBuffer(0) = vFixRepotFiles.SelectedItems(vCnt)
    vBuffer(1) = "FRP_" & fSplit(Dir(vFixRepotFiles.SelectedItems(vCnt)), 1, ".")
    Call SelectOrAddWs(vBuffer(1), ThisWorkbook.Name)

    With ThisWorkbook.Sheets(vBuffer(1))
        .Activate
        .Cells.Clear
        .Range(Cells(1, 1), Cells(1, Columns("S").Column)).Value2 = Split("_Index,Object,Top,Left,Width,Height,LibraryComment,_Text,_TagIn,_TagInColor,_TagInVisible,_OpenPic,_Run,_Mes1,_Mes2,_Img,_Hi,_Lo,_Del", ",")
        Call downloadFixReport(vBuffer(0), vBuffer(1))
        Set vDomDoc = vDomDoc.DocumentElement.ChildNodes(0).ChildNodes(0).appendChild(CreapteFixReportForm(.Name))
    End With
Next vCnt

vBuffer(2) = fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_PATH_OUTXML", sLinkShtName)
Call SaveTextWithUtf8(vDomDoc.XML, vBuffer(2))
Call DebugOut("UTIL", "FIXREPO_EXE_CREATESHT :" & vBuffer(2))

Set vDom = Nothing
Set vDomDoc = Nothing

If CBool(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_SORTSHT", sLinkShtName)) = True Then SortShtByPGPRM
If CBool(fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_RTNSHT", sLinkShtName)) = True Then ThisWorkbook.Sheets(sLinkShtName).Activate
Application.ScreenUpdating = True
End Sub



Sub uploadFixReportB()
Application.ScreenUpdating = False
Application.DisplayAlerts = False
Dim vDom As clsXapDebug: Set vDom = New clsXapDebug
Dim vFiles As FileDialog: Set vFiles = Application.FileDialog(msoFileDialogFilePicker)
Dim vSelectFiles As FileDialogSelectedItems
Dim vCnt As Long
Dim vBuffer(10) As String
Dim wb As Workbook

With vFiles
    .Filters.Clear
    .AllowMultiSelect = True
    .InitialFileName = fGetPgVal("fix_repo_dir")
    .Show
End With

On Error Resume Next
If vFiles.SelectedItems.Count = 0 Then Exit Sub: Exit Sub
Set wb = Workbooks.Add: wb.Activate

For vCnt = 1 To vFiles.SelectedItems.Count ' 1 base array
    With wb.Worksheets.Add
        .Range(Cells(1, 1), Cells(1, Columns("S").Column)).Value2 = Split("_Index,Object,Top,Left,Width,Height,LibraryComment,_Text,_TagIn,_TagInColor,_TagInVisible,_OpenPic,_Run,_Mes1,_Mes2,_Img,_Hi,_Lo,_Del", ",")
        vBuffer(0) = fSetPgVal("fix_repo_InPath", vFiles.SelectedItems(vCnt))
        vBuffer(0) = fSetPgVal("fix_repo_OutPath", fReplace(vFiles.SelectedItems(vCnt), ".ODR", ".xml"))
        vDom.reloadPath
        vDom.downloadFixReport
        .Name = fSplit(Dir(vFiles.SelectedItems(vCnt)), 1, ".")
        vDom.uploadFixFilterXml
    End With
Next vCnt
wb.Sheets("Sheet1").Delete

Set vDom = Nothing
Application.ScreenUpdating = True
Application.DisplayAlerts = True
End Sub


Sub uploadFixFormXml()
Dim vWs As Worksheet: Set vWs = ActiveSheet
Dim vDom As clsXapDomDoc: Set vDom = vDom
Dim lOdfH As Long: lOdfH = 10000
Dim lOdfW As Long: lOdfW = 10000
Dim vForm As IXMLDOMElement: Set vForm = vDom.createFormElm(lOdfH, lOdfW)
Dim vFilter As IXMLDOMElement
Dim lCol As Long: lCol = 1
Dim s As String
Dim sKey As String
Dim lMatchCol As Long

With vWs
    .Range("A1").Interior.Color = RGB(255, 155, 155)
    .Activate
    If .Range("A2").Value = "" Then .Range("A3").Activate Else .Range("A2").Activate

    Do While ActiveCell.Value <> "" ' row direction
        .Range("A1").Interior.Color = RGB(55, 155, 255)
        Set vFilter = setBaseRect

        Do While .Cells(1, lCol).Value <> "" 'field Attrs <--->
            Select Case .Cells(1, lCol).Value
                Case "_Index"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                        If fGetPgVal("fix_repo_isIndex") = 1 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Top"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Left"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Width"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Height"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "LibraryComment"
                        Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_TagInColor"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If fGetPgVal("fix_repo_isIndex") = 0 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")

                Case "_TagInVisible"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If fGetPgVal("fix_repo_isIndex") = 0 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                    
                Case "_TagIn"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If fGetPgVal("fix_repo_isIndex") = 0 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")

                Case "_OpenPic"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Run"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Text"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = setFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes1"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Img"
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then
                        Set vFilter = vDom.setAttributesEx(vFilter, "clid", "Image")
                        Set vFilter = addFilterText(vFilter, "FileNormal", fReplace(fReplace(FixRepoOutPath_, Dir(FixRepoOutPath_), CStr(.Cells(ActiveCell.Row, lCol).Value)), ".ODF", ".jpg"))
                    End If
                Case "_Hi"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If fGetPgVal("fix_repo_isIndex") = 0 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Lo"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If fGetPgVal("fix_repo_isIndex") = 0 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Del"
                    If .Cells(ActiveCell.Row, lCol).Value = 1 Then
                        Set vFilter = vDom.setAttributesEx(vFilter, "FillColor", "#7F7F7F")
                        Set vFilter = vDom.setAttributesEx(vFilter, "TextColor", "#5A5A5A")
                        Set vFilter = vDom.setAttributesEx(vFilter, "BorderColor", "#5A5A5A")
                    End If
           
           End Select
            
                lCol = lCol + 1

          Loop ' field Attrs
          
        vForm.appendChild vFilter
        lCol = 1

        ActiveCell.Offset(1, 0).Activate 'row direction
    Loop

    .Range("A1").Activate 'iterate end
End With

s = "<?xml version=""1.0""?>" & _
"<ROOT><FORMROOT clid=""Client"" Name=""Client"" ShowGrid=""F"" AdjustGrid=""F"" Product=""PA-Panel"" Ver=""6.0"" Rev=""12.1"" FileRev=""3"" FileSaveWithoutDefault=""T"">" & _
"<OBJ clid=""Folder"" Name=""Folder01"">" & _
"PLACEHOLDER" & _
"</OBJ>" & _
"</FORMROOT></ROOT>"

s = Replace(s, "PLACEHOLDER", vForm.XML)

Set vDom = Nothing
Set vDoc = Nothing
Set vForm = Nothing
Set vFilter = Nothing

End Sub


Sub downloadFixReport(inptRepoPath As String, inptWsName As String) ' report to Excel Sheet
Dim sPathIn As String: sPathIn = inptRepoPath
Dim oArr As Object: Set oArr = CreateObject("System.Collections.ArrayList")
Dim sLine As String
Dim vCol As Long: vCol = 2
Dim vRplAttr As String
Dim s As String
Dim sKey As String
Dim l As Long: l = 1 'uniqueIdNum
Dim lMatchCol As Long 'headerField

With Sheets(inptWsName)
    .Range(Cells(2, 1), Cells(Rows.Count, Columns.Count)).Clear
    .Range(Cells(1, 1), Cells(Rows.Count, Columns.Count)).NumberFormatLocal = "@"
    .Range("A2").Activate 'start position
    .Range("A1").Interior.Color = RGB(255, 155, 155)

    Open sPathIn For Input As #1
        Do Until EOF(1)
        Line Input #1, sLine
        
    Call DebugOut("REM", "If sLine =  Then Stop")

    If fSplit(Trim(sLine), 1, ":") = "ﾋﾟｸﾁｬ名" Then
            sKey = "Object": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "ODF"
            sKey = "Top": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = 0
            sKey = "Left": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = 0
            sKey = "Height": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = 10000
            sKey = "Width": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = 10000
            sKey = "_Img": lMatchCol = searchActiveSheetHeaderCol(sKey): If lMatchCol <> 0 And Right(Trim(fSplit(sLine, 2, ":")), 3) = "ODF" Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = Trim(fSplit(sLine, 2, ":"))
    End If
    
    Select Case fSplit(sLine, 11, " ")
        Case "前景色"
            s = "前景色"
        Case "見える/見えない"
            s = "見える/見えない"
        Case "境界色"
            s = "境界色"
    End Select

    'next row(オブジェクト）
    If fSplit(sLine, 1, ":") = "ｵﾌﾞｼﾞｪｸﾄ" Then
        .Range("A1").Interior.Color = RGB(55, 155, 255)
        If Not WorksheetFunction.CountA(Range(Cells(ActiveCell.Row, Columns("H").Column), Cells(ActiveCell.Row, Columns.Count))) = 0 Then
            .Cells(ActiveCell.Row, searchActiveSheetHeaderCol("_Index")).Value = l
            l = l + 1
            ActiveCell.Offset(1, 0).Activate
        End If

        vCol = 2
        Do While .Cells(1, vCol).Value <> "" 'field Attrs <--->
                vRplAttr = .Cells(1, vCol).Value 'each field
                    Select Case vRplAttr
                        Case "Object"
                            .Cells(ActiveCell.Row, vCol).Value = Trim(fSplit(sLine, 2, " ")) ' object name 長方形 楕円 ...
                        Case "Left"
                            .Cells(ActiveCell.Row, vCol).Value = fReplace(fSplit(sLine, 7, " "), ",", "")
                        Case "Top"
                            .Cells(ActiveCell.Row, vCol).Value = fReplace(fSplit(sLine, 8, " "), ",", "")
                        Case "Width"
                           .Cells(ActiveCell.Row, vCol).Value = fReplace(fSplit(sLine, 9, " "), ",", "") - fReplace(fSplit(sLine, 7, " "), ",", "")
                        Case "Height"
                            .Cells(ActiveCell.Row, vCol).Value = fReplace(fSplit(sLine, 10, " "), ",", "") - fReplace(fSplit(sLine, 8, " "), ",", "")
                    End Select
            vCol = vCol + 1
        Loop ' field Attrs
        vCol = 2

    'same row(Parameter)
    ElseIf Trim(fSplit(sLine, 1, ":")) = "ﾀｸﾞ名" Then

            If Not fSplit(sLine, 1, "#") = sLine Then
                    sKey = "_TagIn"
                    lMatchCol = searchActiveSheetHeaderCol(sKey)
                    If lMatchCol <> 0 Then .Cells(ActiveCell.Row, lMatchCol).Value = Trim(fSplit(sLine, 2, "#"))
                ElseIf s = "前景色" Or s = "境界色" Then
                    sKey = "_TagInColor"
                    sLine = fSplit(sLine, 3, ":"): sLine = fSplit(sLine, 1, "."): sLine = Trim(sLine)
                    lMatchCol = searchActiveSheetHeaderCol(sKey)
                    If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ", ") & sLine

                ElseIf s = "見える/見えない" Then
                    sKey = "_TagInVisible"
                    sLine = fSplit(sLine, 3, ":"): sLine = fSplit(sLine, 1, "."): sLine = Trim(sLine)
                    lMatchCol = searchActiveSheetHeaderCol(sKey)
                    If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ", ") & sLine
                Else
                    Stop 'new property found
            End If

        ElseIf Trim(fSplit(Trim(sLine), 1, ":")) = "ﾃｷｽﾄ" Then
            sKey = "_Text"
            sLine = fSplit(Trim(sLine), 2, ":")
            lMatchCol = searchActiveSheetHeaderCol(sKey)
            If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ", ") & sLine

        ElseIf Trim(fSplit(sLine, 1, ":")) = "TOGGLEDIG THISNODE" Or _
            Trim(fSplit(sLine, 1, ":")) = "SETVAL THISNODE" Or _
            Trim(fSplit(sLine, 1, ":")) = "OFFSCAN THISNODE" Or _
            Trim(fSplit(sLine, 1, ":")) = "CLOSEDIG THISNODE" Or _
            Trim(fSplit(sLine, 1, ":")) = "OPENDIG THISNODE" Then
                sKey = "_TagIn"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then
                    If Not fSplit(sLine, 1, "#") = sLine Then
                        .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = Trim(fSplit(sLine, 2, "#"))
                    Else
                        sLine = Trim(fSplit(sLine, 1, ":")) & " : " & Trim(fSplit(fSplit(sLine, 2, ":"), 1, "."))
                        .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
                    End If
                End If
            
            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "OPENPIC" Then
                sLine = fSplit(Trim(sLine), 2, " ")
                sKey = "_OpenPic"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
            
            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "REPLACEPIC" Then
                sLine = fSplit(Trim(sLine), 3, " ")
                sKey = "_OpenPic"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
            
            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "RUNTASK" Then
                sLine = WorksheetFunction.Substitute(Trim(sLine), "RUNTASK", " ")
                sKey = "_Run"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
            
            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "#MES1" Then
                sLine = fSplit(Trim(sLine), 3, " ")
                sKey = "_Mes1"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
            
            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "#MES2" Then
                sLine = fSplit(Trim(sLine), 3, " ")
                sKey = "_Mes2"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine

            ElseIf Trim(fSplit(Trim(sLine), 1, " ")) = "#IN_MAX" Then
                sLine = fSplit(fReplace(sLine, " ", ""), 2, "=")
                sKey = "_Hi"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine

            ElseIf Trim(fSplit(Trim(sLine), 1, "=")) = "#IN_MIN" Then
                sLine = fSplit(fReplace(sLine, " ", ""), 2, "=")
                sKey = "_Lo"
                lMatchCol = searchActiveSheetHeaderCol(sKey)
                If lMatchCol <> 0 Then .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = .Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value & IIf(.Cells(ActiveCell.Row, searchActiveSheetHeaderCol(sKey)).Value = "", "", ",") & sLine
            End If
    
        Loop 'next line
    Close #1
                .Cells(ActiveCell.Row, searchActiveSheetHeaderCol("_Index")).Value = l
End With

End Sub


'REPO SHT => ELM VER
Function CreapteFixReportForm(inptSht As String) As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim sWsLink As String: sWsLink = "LinkShtClt"
Dim lOdfH As Long: lOdfH = CLng(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM-HEIGHT", sWsLink))
Dim lOdfW As Long: lOdfW = CLng(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM-WIDTH", sWsLink))
Dim vForm As IXMLDOMElement: Set vForm = vDom.createFormElm(lOdfH, lOdfW, inptSht)
Dim vFilter As IXMLDOMElement
Dim lCol As Long: lCol = 2
Dim sKey As String
Dim vBuffer(10) As String
Dim lMatchCol As Long

With ThisWorkbook.Sheets(inptSht)
    .Activate
    .Range("A1").Interior.Color = RGB(255, 155, 155)
    If .Range("A2").Value = "" Then .Range("A3").Activate Else .Range("A2").Activate

    Do While ActiveCell.Value <> "" ' row direction
        .Range("A1").Interior.Color = RGB(55, 155, 255)
        Set vFilter = setBaseRect
       
        Do While .Cells(1, lCol).Value <> "" 'field Attrs <--->
            Select Case .Cells(1, lCol).Value
                Case "_Index"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                        If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Top"
                        vBuffer(0) = (.Cells(ActiveCell.Row, lCol).Value / 100) * CInt(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM_RESCALE", sWsLink))
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(vBuffer(0)))
                Case "Left"
                        vBuffer(0) = (.Cells(ActiveCell.Row, lCol).Value / 100) * CInt(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM_RESCALE", sWsLink))
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(vBuffer(0)))
                Case "Width"
                        vBuffer(0) = (.Cells(ActiveCell.Row, lCol).Value / 100) * CInt(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM_RESCALE", sWsLink))
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(vBuffer(0)))
                Case "Height"
                        vBuffer(0) = (.Cells(ActiveCell.Row, lCol).Value / 100) * CInt(fKey2Val("PGPRM_CLT_EXE_ROOT", "FIXREPO_FORM_RESCALE", sWsLink))
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(vBuffer(0)))
                Case "LibraryComment"
                        Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_TagInColor"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_TagInVisible"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_TagIn"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_OpenPic"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Run"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Text"
                
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = setFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    
                Case "_Mes1"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Img"
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then
                    Call DebugOut("REM", "not yet")
'                        Set vFilter = vdom.setAttributesEx(vFilter, "clid", "Image")
'                        Set vFilter = addFilterText(vFilter, "FileNormal", fReplace(fReplace(FixRepoOutPath_, Dir(FixRepoOutPath_), CStr(.Cells(ActiveCell.Row, lCol).Value)), ".ODF", ".jpg"))
                    End If
                Case "_Hi"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Lo"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Del"
                    If .Cells(ActiveCell.Row, lCol).Value = 1 Then
                        Set vFilter = vDom.setAttributesEx(vFilter, "FillColor", "#7F7F7F")
                        Set vFilter = vDom.setAttributesEx(vFilter, "TextColor", "#5A5A5A")
                        Set vFilter = vDom.setAttributesEx(vFilter, "BorderColor", "#5A5A5A")
                    End If
           End Select
                lCol = lCol + 1
          Loop ' field Attrs
        vForm.appendChild vFilter
        lCol = 1
        ActiveCell.Offset(1, 0).Activate 'row direction
    Loop
    .Range("A1").Activate 'iterate end
End With

Set CreapteFixReportForm = vForm

Set vDom = Nothing
Set vDoc = Nothing
Set vForm = Nothing
Set vFilter = Nothing

End Function

'SHT => ONE FILE VER
Sub uploadFixFilterXml(inptSht As String)
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim lOdfH As Long: lOdfH = 10000
Dim lOdfW As Long: lOdfW = 10000
Dim vForm As IXMLDOMElement: Set vForm = vDom.createFormElm(lOdfH, lOdfW)
Dim vFilter As IXMLDOMElement
Dim lCol As Long: lCol = 2
Dim s As String
Dim sKey As String
Dim lMatchCol As Long

With ThisWorkbook.Sheets(inptSht)
    .Activate
    .Range("A1").Interior.Color = RGB(255, 155, 155)
    If .Range("A2").Value = "" Then .Range("A3").Activate Else .Range("A2").Activate

    Do While ActiveCell.Value <> "" ' row direction
        .Range("A1").Interior.Color = RGB(55, 155, 255)
        Set vFilter = setBaseRect

        Do While .Cells(1, lCol).Value <> "" 'field Attrs <--->
            Select Case .Cells(1, lCol).Value
                Case "_Index"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                        If fGetPgVal("fix_repo_isIndex") = 1 And .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Top"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Left"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Width"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "Height"
                        Set vFilter = setFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "LibraryComment"
                        Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_TagInColor"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_TagInVisible"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_TagIn"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    Set vFilter = setFilterBorder(vFilter)
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "TagIn")
                Case "_OpenPic"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Run"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Text"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = setFilterText(vFilter, "Text", CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes1"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Mes2"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                Case "_Img"
                    If .Cells(ActiveCell.Row, lCol).Value <> "" Then
                        Set vFilter = vDom.setAttributesEx(vFilter, "clid", "Image")
                        Set vFilter = addFilterText(vFilter, "FileNormal", fReplace(fReplace(FixRepoOutPath_, Dir(FixRepoOutPath_), CStr(.Cells(ActiveCell.Row, lCol).Value)), ".ODF", ".jpg"))
                    End If
                Case "_Hi"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Lo"
                    Set vFilter = addFilterText(vFilter, .Cells(1, lCol).Value, CStr(.Cells(ActiveCell.Row, lCol).Value))
                     If .Cells(ActiveCell.Row, lCol).Value <> "" Then Set vFilter = addFilterText(vFilter, "LibraryComment", "HL")
                Case "_Del"
                    If .Cells(ActiveCell.Row, lCol).Value = 1 Then
                        Set vFilter = vDom.setAttributesEx(vFilter, "FillColor", "#7F7F7F")
                        Set vFilter = vDom.setAttributesEx(vFilter, "TextColor", "#5A5A5A")
                        Set vFilter = vDom.setAttributesEx(vFilter, "BorderColor", "#5A5A5A")
                    End If
           End Select
            
                lCol = lCol + 1

          Loop ' field Attrs
          
        vForm.appendChild vFilter
        lCol = 1

        ActiveCell.Offset(1, 0).Activate 'row direction
    Loop

    .Range("A1").Activate 'iterate end
End With

s = "<?xml version=""1.0""?>" & _
"<ROOT><FORMROOT clid=""Client"" Name=""Client"" ShowGrid=""F"" AdjustGrid=""F"" Product=""PA-Panel"" Ver=""6.0"" Rev=""12.1"" FileRev=""3"" FileSaveWithoutDefault=""T"">" & _
"<OBJ clid=""Folder"" Name=""Folder01"">" & _
"PLACEHOLDER" & _
"</OBJ>" & _
"</FORMROOT></ROOT>"

s = Replace(s, "PLACEHOLDER", vForm.XML)

Call SaveTextWithUtf8(s, FixRepoOutPath_)

Set vDom = Nothing
Set vDoc = Nothing
Set vForm = Nothing
Set vFilter = Nothing

End Sub


Sub ★XmlUtil(): End Sub


Function getFolder(inptName As String) As IXMLDOMSelection 'as Alias
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vRslt As IXMLDOMSelection
Set vRslt = vDom.selectNodesEX("//FORMROOT/OBJ[@clid='Folder'][@Name='" & inptName & "']")
Set getFolder = vRslt
Set vRslt = Nothing
End Function


Function setBaseNumEll() As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vFilter As IXMLDOMElement
'<OBJ clid="Ellipse" Text="1" TextFont="HGPｺﾞｼｯｸE,-8,0,400,0,0,0,0,0" RotateX="339" RotateY="100" Top="90" Left="329" Height="21" Width="21" LibraryComment="p_dbgNumber">
With vDom
Set vFilter = .createEllipseElm(0, 0, 0, 0)
Set vFilter = .setAttributesEx(vFilter, "LibraryComment", "p_dbgNumEll")
Set vFilter = .setAttributesEx(vFilter, "Text", "")
Set vFilter = .setAttributesEx(vFilter, "Width", "20")
Set vFilter = .setAttributesEx(vFilter, "Height", "20")
Set vFilter = .setAttributesEx(vFilter, "TextFont", "HGPｺﾞｼｯｸE,-8,0,700,0,0,0,0,0")
Set vFilter = .setAttributesEx(vFilter, "Customize", _
"[Property]" & vbLf & _
"_Index=[string]" & vbLf & _
"_Comment1=[string]")
End With

Set setBaseNumEll = vFilter
Set vDom = Nothing
Set vFilter = Nothing

End Function


Function setBaseRect() As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vFilter As IXMLDOMElement
'red filter elem

With vDom
Set vFilter = .createRedFilter(0, 0, 0, 0)
Set vFilter = .setAttributesEx(vFilter, "LibraryComment", "p_FixFil")
Set vFilter = .setAttributesEx(vFilter, "Customize", _
"[Property]" & vbLf & _
"_Index=[string]" & vbLf & _
"_TagInColor=[string]" & vbLf & _
"_TagInVisible=[string]" & vbLf & _
"_TagIn=[string]" & vbLf & _
"_OpenPic=[string]" & vbLf & _
"_Run=[string]" & vbLf & _
"_Text=[string]" & vbLf & _
"_Mes1=[string]" & vbLf & _
"_Mes2=[string]" & vbLf & _
"_Hi=[string]" & vbLf & _
"_Lo=[string]" & vbLf & _
"_Comment1=[string]")
End With

Set setBaseRect = vFilter
Set vFilter = Nothing
Set vDom = Nothing

End Function


Function setFilterBorder(inptElm As IXMLDOMElement) As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vFilter As IXMLDOMElement: Set vFilter = inptElm
'rem

With vDom
Set vFilter = .setAttributesEx(vFilter, "TextFont", "Meiryo UI,-16,0,700,0,0,0,0,0")
Set vFilter = .setAttributesEx(vFilter, "TextColor", "#800000")
Set vFilter = .setAttributesEx(vFilter, "FillColor", "#FFFFFF")
Set vFilter = .setAttributesEx(vFilter, "Border", "True")
Set vFilter = .setAttributesEx(vFilter, "BorderColor", "#800000")
Set vFilter = .setAttributesEx(vFilter, "BorderWidth", "2")
End With

Set setFilterBorder = vFilter
Set vFilter = Nothing
Set vDom = Nothing

End Function


Function setFilterText(inptElm As IXMLDOMElement, inptAtr As String, inptValue As String) As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vFilter As IXMLDOMElement: Set vFilter = inptElm
Dim sValue As String: sValue = inptValue

With vDom
If Not IsNull(vFilter.getAttribute(inptAtr)) And sValue <> "" Then
    Set vFilter = .setAttributesEx(vFilter, inptAtr, sValue)
ElseIf sValue <> "" Then
    Set vFilter = .setAttributesEx(vFilter, inptAtr, sValue)
Else
    'usual
End If
End With

Set setFilterText = vFilter
Set vFilter = Nothing
Set vDom = Nothing

End Function


Function addFilterText(inptElm As IXMLDOMElement, inptAtr As String, inptValue As String) As IXMLDOMElement
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
Dim vFilter As IXMLDOMElement: Set vFilter = inptElm
Dim sValue As String: sValue = inptValue

With vDom
If Not IsNull(vFilter.getAttribute(inptAtr)) And sValue <> "" Then
    Set vFilter = .setAttributesEx(vFilter, inptAtr, .getAttributesEx(vFilter, inptAtr) & ", " & sValue)
ElseIf sValue <> "" Then
    Set vFilter = .setAttributesEx(vFilter, inptAtr, sValue)
Else
    'usual
End If
End With

Set addFilterText = vFilter
Set vFilter = Nothing
Set vDom = Nothing

End Function


Sub delUnMatchRow(inptCol As Long, inptCnd As String)
Dim sDelCol As String: sDelCol = inptCnd
With ActiveSheet
    .Cells(.Cells(Rows.Count, 1).End(xlUp).Row, 1).Activate
    Do Until ActiveCell.Row = 1
        If .Cells(ActiveCell.Row, inptCol) <> inptCnd Then
            ActiveCell.EntireRow.Select
            Selection.Delete Shift:=xlUp
        End If
        ActiveCell.Offset(-1, 0).Activate
    Loop
End With
End Sub


Sub delUnMatchForm(inptCol As Long, inptCnd As String)
Dim sMatch As String: sMatch = inptCnd
Dim l As Long

With ActiveSheet
    .Cells(2, 1).Activate
    Do Until .Cells(ActiveCell.Row, inptCol).Value = inptCnd
        ActiveCell.EntireRow.Select
        Selection.Delete Shift:=xlUp
        If ActiveCell.Row = .Cells(.Cells(Rows.Count, 1).End(xlUp).Row, 1).Row + 1 Then Exit Do
    Loop

    If .Cells(ActiveCell.Row, inptCol).Value = sMatch Then
        ActiveCell.Offset(1, 0).Activate
        Do Until .Cells(ActiveCell.Row, inptCol).Value <> sMatch
            ActiveCell.Offset(1, 0).Activate
        Loop
        
        Do Until ActiveCell.Value = ""
            ActiveCell.EntireRow.Select
            Selection.Delete Shift:=xlUp
        Loop
    End If
End With

End Sub


Function searchActiveSheetHeaderCol(inptKey As String) As Long
Dim sWbName As String:  sWbName = ActiveWorkbook.Name
Dim sWsName As String:  sWsName = ActiveSheet.Name
Dim sKey As String: sKey = inptKey
Dim sMatchRow As String
Dim sRslt As String
On Error Resume Next

With Workbooks(sWbName).Worksheets(sWsName)
    Dim rRow As Range
    Set rRow = .Rows(1)
    If rRow Is Nothing Then Stop ' "Book or Sheet name is wrong"
        sMatchRow = Application.WorksheetFunction.Match(sKey, rRow, 0)
        If sMatchRow <> "" Then
                sRslt = sMatchRow
            Else
                sRslt = ""
        End If
End With

searchActiveSheetHeaderCol = CLng(sRslt)

End Function


Sub loadXml()
Dim vDom As clsXapDomDoc: Set vDom = New clsXapDomDoc
    Call vDom.loadClientFile(InPath_)
End Sub


Sub reloadPath(Optional inptFixRepoInPath As String _
                        , Optional inptFixRepoOutPath As String)
    InPath_ = fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_PATH_LOAD", "LinkShtClt")
    OutPath_ = fKey2Val("PGPRM_CLT_EXE_ROOT", "XML_PATH_SAVE", "LinkShtClt")
    
    'alter repopath => batch
    FixRepoInPath_ = inptFixRepoInPath
    FixRepoOutPath_ = inptFixRepoOutPath

    Call DebugOut("XML_PROC", _
                        "sub reloadpath " & vbNewLine & _
                        "Inpath " & InPath_ & vbNewLine & _
                        "Outpath " & OutPath_)
    loadXml
End Sub



Sub CnvFixDB2FapCSV()
Application.ScreenUpdating = False
Call fOpenDB

Dim l As Long

Dim cSortWsName As String: cSortWsName = "Alias"
Dim cOtptWsName As String: cOtptWsName = "Server"

'Dim cWsDB As Worksheet: Set cWsDB = ThisWorkbook.Sheets("testCase")
Dim cWsDB As Worksheet: Set cWsDB = Workbooks(fGetDsWb()).Sheets(fGetDsWs())

Dim cWsSort As Worksheet
Dim cWsOtpt As Worksheet

Dim cPlcPrefix As String: cPlcPrefix = "PLC"

Dim s As String
Dim vPrv As Date

'sht copy
Dim cRowDbSt As String: cRowDbSt = "A6"
Dim cColDbAtag As Long: cColDbAtag = Range(gcColA_TAG & "1").Column

Dim cColSortAlias As Long: cColSortAlias = 1
Dim cColSortPlc As Long: cColSortPlc = 2

'convert
Dim vPrvPlcName As String: vPrvPlcName = ""
Dim vCrrPlcName As String: vCrrPlcName = ""
Dim vCrrAliasName As String: vCrrAliasName = ""

Dim vRowSort As Long: vRowSort = 1
Dim vRowOtpt As Long: vRowOtpt = 1

Dim vLoopRng As Range

Debug.Print "Start Time: ", Format(Now, "hh:mm:ss"): vPrv = Now

''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
With Application.Workbooks.Add
    Set cWsOtpt = Workbooks(.Name).Sheets(1)
    cWsOtpt.Name = cOtptWsName
    Set cWsSort = Workbooks(.Name).Sheets.Add
    cWsSort.Name = cSortWsName
End With

'copy column(A:Alias, B:PLC)
With cWsSort

For l = cWsDB.Range(CStr(cRowDbSt)).Row To cWsDB.Cells(cWsDB.Rows.Count, 2).End(xlUp).Row 'avoid End

    s = cWsDB.Cells(l, cColDbAtag).Value 'Alias W0001_PV
    If Left(fGetFixPlc(s), 3) = cPlcPrefix Then
            .Cells(l - 1, cColSortAlias).Value = s 'l - header(1)
            .Cells(l - 1, cColSortPlc).Value = fGetFixPlc(s) 'l - header(1) AFTER DELETE
            .Cells(l - 1, cColSortPlc + 1).Value = GetFapPlc6notDBFol1(s) 'l - header(1)
        
        ElseIf fGetFixIODV(s) = "SIM" Then ' SIM => MEM
            .Cells(l - 1, cColSortAlias).Value = s 'l - header(1)
            .Cells(l - 1, cColSortPlc).Value = "ZMem" 'l - header(1)
            .Cells(l - 1, cColSortPlc + 1).Value = "Mem.F01." & checkNumSt(s) 'l - header(1)
            
        ElseIf fGetFixType(s) = "BL" Or _
                fGetFixType(s) = "CA" Or _
                fGetFixType(s) = "PG" Then
            .Cells(l - 1, cColSortAlias).Value = s 'l - header(1)
            .Cells(l - 1, cColSortPlc).Value = "ZMem" 'l - header(1)
            .Cells(l - 1, cColSortPlc + 1).Value = "Mem.F01." & checkNumSt(s)  'l - header(1)
        Else
        ' ETR validation
        End If
Next l

'>>column(B)/PLC sort
.Columns("A:C").Sort key1:=.Columns("B"), order1:=xlAscending

Do Until .Cells(vRowSort, cColSortAlias).Value = ""

        vCrrPlcName = fSplitByDelimiter(.Cells(vRowSort, cColSortPlc).Value, ":", 1) 'PLC01
        'overWrite  eg ZMem => Mem
        If Left(.Cells(vRowSort, cColSortPlc).Value, 1) = "Z" Then
            vCrrPlcName = Right(vCrrPlcName, Len(vCrrPlcName) - 1)
        End If
          
        vCrrAliasName = .Cells(vRowSort, cColSortAlias).Value
        
        'csv1
            cWsOtpt.Cells(vRowOtpt, 1).Value = cnvPlc2Csv1(vCrrPlcName) 'PLC01=>CSV1

            'overWrite UNIT => *
            If vCrrPlcName = vPrvPlcName Then
                    cWsOtpt.Cells(vRowOtpt, 1).Value = "*"
            End If
        
            'csv2
            cWsOtpt.Cells(vRowOtpt, 2).Value = "F01#" & cnvPlc2StEnd(vCrrPlcName)
                        
            'overWrite UNIT => *
            If vCrrPlcName = vPrvPlcName Then
                    cWsOtpt.Cells(vRowOtpt, 2).Value = "*"
            End If
                
        'csv3
        If Left(.Cells(vRowSort, cColSortPlc).Value, 3) = cPlcPrefix Then
            cWsOtpt.Cells(vRowOtpt, 3).Value = CnvFixAlias2FapCSV3(vCrrAliasName)
        Else ' MEM
             cWsOtpt.Cells(vRowOtpt, 3).Value = CnvFixSIMAlias2FapCSV3(vCrrAliasName)
        End If
        
        vPrvPlcName = vCrrPlcName
        vRowOtpt = vRowOtpt + 1
        vRowSort = vRowSort + 1
    Loop
End With


With cWsSort
    For Each vLoopRng In .Range("A:A")
        If vLoopRng.Value <> "" Then
            vLoopRng.Value = checkNumSt(vLoopRng.Value)
        End If
    Next vLoopRng

    'Delete
    .Columns("B").Delete Shift:=xlToLeft
End With

With cWsOtpt
End With

Debug.Print "finish:", Format(Now, "hh:mm:ss"), "Time:", Format(Now - vPrv, "hh:mm:ss")

Call fCloseDB
Application.ScreenUpdating = True
End Sub



